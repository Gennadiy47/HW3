// Задача 3. Ввести три числа и определить, верно ли, что они вводились в порядке
//возрастания.

int main(int argc, const char * argv[]){
    setlocale(LC_CTYPE,"Russian");
    int a,b,c;
    printf("Введите три целых числа в порядке возрастания:\n");
    scanf("%d%d%d",&a,&b,&c);

    if (a < b,b < c,a < c){
        printf("YES");
    }
        else
            {
             printf("NO");
        }

    return 0;
    }


